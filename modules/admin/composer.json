{
    "name": "docotel/yii2-dcms",
    "description": "RBAC, CMS Auth manager for Yii2 ",
    "keywords": ["yii", "admin", "auth", "rbac", "cms"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "http://gitlab.docotel.net/docotel/yii2-dcms/issues",
        "source": "http://gitlab.docotel.net/docotel/yii2-dcms.git"
    },
    "authors": [
        {
            "name": "Docotel",
            "email": "dev@docotel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "yiisoft/yii2": "~2.0.7"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "~2.0"
    },
    "suggest": {
        "yiisoft/yii2-bootstrap": "Used when using different layout"
    },
    "autoload": {
        "psr-4": {
            "docotel\\dcms\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
